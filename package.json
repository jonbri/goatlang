{
  "name": "goatlang",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonbri/goatlang.git"
  },
  "scripts": {
    "start": "node index.js",
    "start:go": "go run app/hello-world.go",
    "test": "prettier --check \"**/*.{js,md}\" && go version && ./bin/gotagger --version && yarn next",
    "format": "prettier --write \"**/*.{js,md}\"",
    "build": "cp index.js build",
    "next": "./bin/gotagger",
    "changelog": "./bin/changelog.sh"
  },
  "license": "MIT",
  "bin": {
    "goatlang": "index.js"
  },
  "devDependencies": {
    "conventional-changelog": "5.1.0",
    "prettier": "^2.8.4"
  }
}
